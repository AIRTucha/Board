  "dependencies": {
    "elm": "^0.18.0",
    "object-hash": "^1.3.0"
  },
  "devDependencies": {
    "elm-github-install": "^1.6.1",
    "elm-ordeal": "0.0.5",
    "nodemon": "^1.12.1"
  }
}
